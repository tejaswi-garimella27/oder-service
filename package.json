{
  "name": "order-service",
  "version": "1.0.0",
  "description": "This is an order service that allows management of orders and integration of products from multiple external pharmacies. ",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon -r dotenv/config src/app.ts",
    "start": "npm run build && node --env-file=.env dist/app.js",
    "lint": "eslint . --ext .ts",
    "test": "jest  --coverage -c jest.config.js"
  },
  "keywords": [
    "order-service",
    "pharmacies",
    "products"
  ],
  "author": "Tejaswi Garimella",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
